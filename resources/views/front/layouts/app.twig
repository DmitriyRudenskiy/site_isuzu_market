<script>
    $("#modal_callback form, #modal_order form, #modal_find_part form, #modal_get_bonus form").submit(function(){

        var url = "{{ route('front_callback_index') }}";

        $.ajax({
            type: "POST",
            url: url,
            data: $(this).serialize(),
            success: function(data) {
                $("#modal_callback, #modal_order, #modal_find_part, #modal_get_bonus").modal('hide');
                $("#alert_send_message").show();
            }
        });

        return false;
    });
</script>
