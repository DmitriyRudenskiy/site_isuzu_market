<script>
    $(document).ready(function () {
        var isMobile = {
            Android: function () {
                return navigator.userAgent.match(/Android/i);
            },
            BlackBerry: function () {
                return navigator.userAgent.match(/BlackBerry/i);
            },
            iOS: function () {
                return navigator.userAgent.match(/iPhone|iPad|iPod/i);
            },
            Opera: function () {
                return navigator.userAgent.match(/Opera Mini/i);
            },
            Windows: function () {
                return navigator.userAgent.match(/IEMobile/i);
            },
            any: function () {
                return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
            }
        };

        function clear() {
            $(".__effect_zoom_wrap").each(function () {

                $(this).find("img").insertAfter($(this));

                $(this).remove();
            });
        }

        $(window).resize(function () {
            clear()
        });

        var className = "__effect_zoom_wrap";

        if (!isMobile.any()) {
            $("img.__effect_zoom").hover(function () {
                if ($(this).parent().attr("class") !== className) {

                    var parent = $('<div>').addClass(className);
                    parent.width($(this).width());
                    parent.height($(this).height());
                    parent.css("overflow", "hidden");

                    parent.on('mouseover', function () {
                        clear();
                    });

                    $(this).wrap(parent);
                }
            }, clear);
        }
    });
</script>

<script>

    /* Leasing calculator */
    $(document).ready(function () {

        function getInMouth() {

            var price = parseInt($('[name="price"]').val());
            var period = parseInt($('[name="period"]').val()) * 12;
            var advance = parseInt($('[name="advance"]').val());
            var percent =  6.5;

            if (advance < 1) {
                advance = 1;
            }

            console.log(['debug: cal in mouth', price, period, advance]);

            return Math.floor((((price * (percent / 100)) * (period / 12) + price) -  (price / 100 * advance)) / period);
        }


        $('[data-period=' + parseInt($('[name="period"]').val()) + ']' ).css('color', 'red');
        $('[data-advance=' + parseInt($('[name="advance"]').val()) + ']' ).css('color', 'red');


        var element = $(".pay_in_month");

        element.each(function(){
            element.html(getInMouth());
        });

        $('[name="period"]').change(function() {
            $('#preiod_value').text($(this).val() * 12);
            element.html(getInMouth());
        });

        $('[name="advance"]').change(function() {
            $('#advance_value').text($(this).val() * 1);
            element.html(getInMouth());
        });

        /* bottom label */
        $(".__leasing_data_period").click(function(){
            var value = parseInt($(this).attr('data-period'));

            $('[name="period"]').val(value);
            $('[name="period"]').change();

            $(".__leasing_data_period").css('color', 'black');
            $(this).css('color', 'red');
        });

        $(".__leasing_data_advance").click(function(){
            var value = parseInt($(this).attr('data-advance'));

            $('[name="advance"]').val(value);
            $('[name="advance"]').change();

            $(".__leasing_data_advance").css('color', 'black');
            $(this).css('color', 'red');
        });
    });
</script>

